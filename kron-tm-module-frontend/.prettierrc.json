{
	"singleQuote": true,
	"jsxSingleQuote": true,
	"printWidth": 160,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"quoteProps": "as-needed",
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"proseWrap": "preserve",
	"vueIndentScriptAndStyle": true,
	"singleAttributePerLine": false
}
